

import math
import os
import random
import re
import sys

n = int(input ())
arr = list(map(int,input().rstrip().split()))
m =arr[0]
for i in range(0,n):
    if arr[i]> m:
        m =arr[i]
count =0
for j in range(0,n):
    if arr[j] == m:
        count = count +1
print(count)


